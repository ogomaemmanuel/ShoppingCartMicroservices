services:
  basket.data:
    image: redis
    ports:
    - 6379:6379/tcp
  basketservice:
    build:
      context: C:\Users\Emmanuel\source\repos\ShoppingCartVersion2
      dockerfile: BasketService/Dockerfile
    depends_on:
    - basket.data
    - rabbitmq
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: basketservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.0/BasketService.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - 9001:80/tcp
    volumes:
    - C:\Users\Emmanuel\source\repos\ShoppingCartVersion2\BasketService:/app:rw
    - C:\Users\Emmanuel\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
